   "source": [
    "def strStr(haystack, needle):\n",
    "    if len(needle)==0:\n",
    "            return 0\n",
    "    \n",
    "    \n",
    "    if needle in haystack:\n",
    "        return haystack.find(needle)\n",
    "            \n",
    "    return -1"
   ]
  },
